{
  "name": "sheety-localization-example",
  "description": "Example project using sheety-localization to generate localization files",
  "version": "0.0.1",
  "type": "module",
  "main": "index.js",
  "scripts": {
    "dev": "vite",
    "preview": "vite preview",
    "locales:gen": "node scripts/generate.js --credentials ./credentials.json --sheet 1iTmPNGoo41_rk2uLThru1WxbnA-K96_OZmsCdLTcWYw",
    "preinstall": "npx only-allow pnpm",
    "start": "vite --force",
    "start-in-docker": "vite --force --host",
    "serve": "pnpm run build; node server.js",
    "build": "pnpm run generate-changelog && vite build",
    "test": "vitest",
    "whybundled": "pnpm run profile && whybundled stats.json",
